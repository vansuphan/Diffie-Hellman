                        //-             button#button-addon2.btn.btn-outline-secondary(type='button') Send
        section.alert-connect-share-key
            .card
                .card-body
                    h5.card-title Có một kết nối tới bạn
                    a.btn.btn-primary#btn-add-member(href='#') OK

