    body
        section.title-page
            .container
                .header
                    h3.p-4 Demo Diffie-Hellman
            .container
                .title-group
                    .h2 Thành viên nhóm 
                .list-member.d-flex
                    .content-img-member
                        .h3.name Trang Nữ
                        img.nu(src="./img/44.jpg", alt="nu")
                    .content-img-member
                        .h3.name Minh Hoàng
                        img.hoang(src="./img/55.jpg", alt="")
                    .content-img-member
                        .h3.name Minh Thư
                        img.thu(src="./img/22.jpg", alt="")
                    .content-img-member
                        .h3.name Thương Mến
                        img.men(src="./img/33.jpg", alt="")
                    .content-img-member
                        .h3.name Văn Sự
                        img.su(src="./img/77.jpg", alt="")

        section
            .container
                .row
                    .col-md-4.form-chat
                        h3 Client online
                        #id-client.row
                        #id-form-connectId.row
                            .input-group.mb-3
                                input#s-input-connectID.form-control(type='text', placeholder="Input ID", aria-label="Recipient's username", aria-describedby='button-addon2')
                                .input-group-append
                                    button#button-addon2.btn.btn-outline-secondary.s-button-connection(type='button') Connect
                    .col-md-7.form-crew
                        h3#s-name-and-id-user
                        .col-12.s-form-keys
                            h4 Khóa
                            .key-public
                                h5 Số nguyên tố P và phần tử sinh (p,alpha)
                                span.content-key-public none
                            .key-public-2
                                h5 Public Key 
                                span.content-key-public-2 none
                            .key-private
                                h5 Private Key ( Số ngẫu nhiên bí mật )
                                span.content-key-private none
                            .key-serect
                                h5 Serect Key
                                span.content-key-serect none
